                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ManualAddress"
            android:label="Manual Address"
            android:screenOrientation="portrait"
            android:parentActivityName=".Address_view">
            <meta-data
