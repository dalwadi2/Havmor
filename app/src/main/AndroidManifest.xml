        android:icon="@drawable/hvmorlogo"
        android:label="@string/app_name"

        android:theme="@style/AppTheme">
        <activity
            android:name=".SplashScreenActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait">
            <intent-filter>
